{
    "builds": [{
        "src": "olympics/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "olympics/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static$1"
        }
    ],
    "scripts": {
        "build": "./build_file.sh"
    }
}